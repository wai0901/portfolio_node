<!-- Dots indicator -->

<div id="indicator-dots" class="up-down-button">
    <div class="dots">
        <a href="#s0"><span class="dot port d0" title="Porfolio"><i class="fab fa-product-hunt"></i></span></a>
        <% portfolioData.forEach(data => { %>
            <a href="#<%= data.linkId %>"><span class="dot dot-style <%= data.dataDot %>"></span></a>
        <% }) %>
        <a id="goNext" href="#s1"><span class="dot port dDown" title="Check out my porfolio"><i class="fas fa-chevron-circle-down"></i></span></a>
    </div>
</div>